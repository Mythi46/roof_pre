    "# 🚀 エキスパート改良5: TTA + タイル インテリジェント推論\n",
    "def expert_predict(img_path, conf=0.4, use_tta=True, use_tile=True):\n",
    "    \"\"\"エキスパート級インテリジェント推論関数\"\"\"\n",
    "    \n",
    "    predict_args = {\n",
    "        'source': img_path,\n",
    "        'conf': conf,\n",
    "        'iou': 0.45,\n",
    "        'imgsz': IMG_SIZE,           # 統一解像度\n",
    "        'retina_masks': True,        # 高品質マスク\n",
    "        'overlap_mask': True,        # 重複マスク処理\n",
    "        'save': True,                # 結果保存\n",
    "        'line_width': 2,             # 境界線幅\n",
    "        'show_labels': True,         # ラベル表示\n",
    "        'show_conf': True            # 信頼度表示\n",
    "    }\n",
    "    \n",
    "    # エキスパート改良: テスト時強化 (TTA)\n",
    "    if use_tta:\n",
    "        predict_args['augment'] = True\n",
    "        print(\"🔄 TTA有効 - 1-2pt mAP向上可能\")\n",
    "    \n",
    "    # エキスパート改良: タイル推論 (高解像度衛星画像対応)\n",
    "    if use_tile:\n",
    "        predict_args.update({\n",
    "            'tile': True,               # タイル推論有効\n",
    "            'tile_overlap': 0.25        # タイル重複率\n",
    "        })\n",
    "        print(\"🧩 タイル推論有効 - 高解像度衛星画像対応\")\n",
    "    \n",
    "    return trained_model.predict(**predict_args)\n",
    "\n",
    "# インテリジェント推論テスト\n",
    "test_image = \"/content/スクリーンショット 2025-07-23 15.37.31.png\"\n",
    "\n",
    "if os.path.exists(test_image):\n",
    "    print(f\"🔍 エキスパート級インテリジェント推論: {os.path.basename(test_image)}\")\n",
    "    \n",
    "    # インテリジェント推論実行\n",
    "    results = expert_predict(test_image, conf=0.4, use_tta=True, use_tile=True)\n",
    "    \n",
    "    # 推論結果分析\n",
    "    for r in results:\n",
    "        if r.boxes is not None and len(r.boxes) > 0:\n",
    "            classes = r.boxes.cls.cpu().numpy()\n",
    "            confidences = r.boxes.conf.cpu().numpy()\n",
    "            \n",
    "            print(f\"\\n=== 🎯 インテリジェント推論結果統計 ===\")\n",
    "            print(f\"総検出数: {len(classes)}\")\n",
    "            \n",
    "            # クラス別統計\n",
    "            for class_id in np.unique(classes):\n",
    "                class_name = class_names[int(class_id)]\n",
    "                count = np.sum(classes == class_id)\n",
    "                avg_conf = np.mean(confidences[classes == class_id])\n",
    "                weight = cls_weights[int(class_id)]\n",
    "                \n",
    "                print(f\"{class_name:12}: {count:2d}個 | 平均信頼度: {avg_conf:.3f} | 重み: {weight:.2f}\")\n",
    "            \n",
    "            print(f\"\\n💡 エキスパート改良効果:\")\n",
    "            print(f\"   🔄 TTA強化 - 検出安定性向上\")\n",
    "            print(f\"   🧩 タイル推論 - 高解像度処理対応\")\n",
    "            print(f\"   🎯 統一解像度 - 訓練と一致\")\n",
    "        else:\n",
    "            print(\"⚠️ オブジェクト未検出、信頼度閾値を下げる必要があるかもしれません\")\n",
    "else:\n",
    "    print(f\"⚠️ テスト画像が存在しません: {test_image}\")\n",
    "    print(\"💡 テスト画像をアップロードするかパスを修正してください\")"
